@font-face {
    font-family: 'Golos Text';
    src: url('~static/fonts/Golos/GolosText-Medium.eot');
    src: local('Golos Text Medium'), local('GolosText-Medium'),
        url('~static/fonts/Golos/GolosText-Medium.eot?#iefix') format('embedded-opentype'),
        url('~static/fonts/Golos/GolosText-Medium.woff2') format('woff2'),
        url('~static/fonts/Golos/GolosText-Medium.woff') format('woff'),
        url('~static/fonts/Golos/GolosText-Medium.ttf') format('truetype');
    font-weight: 500;
    font-style: normal;
}

